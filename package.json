		"svelte"
	],
	"author": "Deux Huit Huit",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DeuxHuitHuit/tailwindify-props/issues"
	},
	"homepage": "https://github.com/DeuxHuitHuit/tailwindify-props",
	"devDependencies": {
		"prettier": "3.0.3",
		"rollup": "4.1.4",
		"typescript": "5.2.2",
		"vite": "4.4.11",
		"vitest": "0.34.6"
	}
